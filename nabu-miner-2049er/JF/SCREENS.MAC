;******************************************************************
;		 MACROS TO BUILD SCREENS		
;******************************************************************

	;*********************************************************
	;   MACRO TO BUILD CATWALK TABLE ENTRY
	;	PX - X COORDINATE
	;	PY - Y COORDINATE
	;	PJ - NUMBER OF SECTION PAIRS
	;	PP - EXTRA PIECE INDICATOR
	;*********************************************************

	PUTCAT	MACRO	  PX,PY,PJ,PP
		DB	1,PX,PY,PJ,PP
	ENDM

	;*********************************************************
	;   MACRO TO BUILD LADDER TABLE ENTRY
	;	PX  - X COORDINATE
	;	PY  - Y COORDINATE
	;	PJ  - NUMBER OF LADDER RUNG PAIRS
	;	ADY - PIXEL ADJUSTMENT - Y COORDINATE
	;	BY  - BOTTOM Y
	;*********************************************************

	PUTLAD	MACRO	  PX,PY,PJ,ADY,BY
		DB 	2,PX,PY,PJ,ADY,BY
	ENDM

	;*********************************************************
	;   MACROS TO BUILD SLIDE TABLE ENTRIES
	;	PX  - X COORDINATE
	;	PY  - Y COORDINATE
	;	PJ  - NUMBER OF SLIDE SECTIONS
	;	BX  - BOTTOM X
	;	BY  - BOTTOM Y
	;*********************************************************

	PUTSLD	MACRO	  PX,PY,PJ,BX,BY	;SLIDE DOWN
		DB	3,PX,PY,PJ,BX,BY
	ENDM

	PUTSLR	MACRO	  PX,PY,PJ,BX,BY	;SLIDE RIGHT
		DB	4,PX,PY,PJ,BX,BY
	ENDM

	PUTSLL	MACRO	  PX,PY,PJ,BX,BY	;SLIDE LEFT
		DB	5,PX,PY,PJ,BX,BY
	ENDM

	;*********************************************************
	;   MACRO TO BUILD RAMP TABLE ENTRY
	;	PX  - X COORDINATE
	;	PY  - Y COORDINATE
	;	PJ  - NUMBER OF RAMP SECTIONS
	;	PK  - RAMP DIRECTION
	;		PK = 0  (RAMP UP)
	;		PK = 1  (RAMP DOWN)
	;*********************************************************

	PUTRMÐ	MACRÏ	  PX,PY,PJ,PK
		DB	6,PX,PY,PJ,PK
	ENDM

	;*********************************************************
	;   MACRO TO BUILD TREASURE TABLE ENTRY
	;	PX     - X COORDINATE
	;	PY     - Y COORDINATE
	;	PICADD - ADDRESS OF PICTURE  (WORD)
	;	EXD    - X DIMENSION FOR TSEZOT (ERASING OF PICTURE)
	;	EYD    - Y DIMENSION FOR TSEZOT (ERASING OF PICTURE)
	;	TX     - TOP X
	;	TY     - TOP Y
	;	BX     - BOTTOM X
	;	BY     - BOTTOM Y
	;	VAL    - POINTER TO TREASURE VALUE
	;*********************************************************

	PUTTSE	MACRO	  PX,PY,PICADD,EXD,EYD,TX,TY,BX,BY,VAL
		DB	7,PX,PY
		DW	PICADD
		DB	EXD,EYD,TX,TY,BX,BY,VAL
	ENDM

	;*********************************************************
	;   MACRO TO BUILD MONSTER TABLE ENTRY
	;	TY     - TOP Y
	;	LX     - LEFT X
	;	RX     - RIGHT X
	;	PX     - PRESENT X
	;	DIR    - MONSTER DIRECTION
	;	INT    - MONSTER SPEED (INTERVAL)
	;*********************************************************

	MONENT	MACRO	  TY,LX,RX,PX,DIR,INT
		DB 	8,TY,LX,RX,PX,DIR,INT
	ENDM
;
	;*********************************************************
	;   MACRO TO BUILD CHOP TABLE ENTRY
	;	PX     - X COORDINATE
	;	PY     - Y COORDINATE
	;*********************************************************

	CHOP	MACRO	  PX,PY
		DB	9,PX,PY
	ENDM


	;*********************************************************
	;   MACRO TO BUILD TRANSPORTER TABLE ENTRY
	;	PX     - X COORDINATE
	;	PY     - Y COORDINATE
	;*********************************************************

	PUTTRR	MACRO	   PX,PY,NUM
	  	DB	10,PX,PY,NUM
	ENDM


	;*********************************************************
	;   MACRO TO BUILD MOVING PLATFORM TABLE ENTRY
	;	LX     - LEFT X
	;	RX     - RIGHT X
	;	PX     - PRESENT X
	;	TY     - TOP Y
	;	PATT   - PLATFORM PATTERN (SIZE)
	;	SPEED  - PLATFORM SPEED
	;	IDIR   - INITIAL DIRECTION
	;*********************************************************

	PUTMPT	MACRO	   LX,RX,PX,TY,PATT,SPEED,IDIR
		DB	11,LX,RX,PX,TY,PATT,SPEED,IDIR
	ENDM

	;*********************************************************
	;   MACRO TO BUILD FIXED PLATFORM TABLE ENTRY
	;	PX    - X COORDINATE
	;	PY    - Y COORDINATE
	;	PJ    - NUMBER OF SECTIONS
	;*********************************************************

	PUTPLT	MACRO	   PX,PY,PJ
		DB	12,PX,PY,PJ
	ENDM
;
	;*********************************************************
	;   MACRO TO BUILD TRACKS TABLE ENTRY
	;	PX    - X COORDINATE
	;	PY    - Y COORDINATE
	;	PJ    - NUMBER OF SECTIONS
	;*********************************************************

	PUTTRK	MACRO	   PX,PY,PJ
		DB	13,PX,PY,PJ
	ENDM

	;**********************************************************
	;   MACRO TO BUILD WALL TABLE ENTRY
	;	PX    - TOP X
	;	PY    - TOP Y
	;	PBX   - BOTTOM X
	;       PBY   - BOTTOM Y
	;	PJ    - NUMBER OF SECTIONS
	;	PTBLI - TABLE ENTRY INDICATOR
	;***********************************************************

	PUTWAL	MACRO	   PX,PY,PBX,PBY,PJ,PTBLI
		DB	14,PX,PY,PBX,PBY,PJ,PTBLI
	ENDM

	;**********************************************************
	;   MACRO TO BUILD ROOF TABLE ENTRY
	;	PX    - TOP X
	;	PY    - TOP Y
	;	PBX   - BOTTOM X
	;       PBY   - BOTTOM Y
	;	PJ    - NUMBER OF SECTIONS
	;	PTBLI - TABLE ENTRY INDICATOR
	;***********************************************************

	PUTROF	MACRO	   PX,PY,PBX,PBY,PJ,PTBLI
		DB	15,PX,PY,PBX,PBY,PJ,PTBLI
	ENDM

	;*********************************************************
	;   MACRO TO BUILD SPECIAL EFFECTS TABLE ENTRY
	;	PX  =  1  (RADIOACTIVE MARTINI)
	;	    =  2  (RADIOACTIVE WASTE)
	;	    =  3  (PLUNGERS)
	;*********************************************************

	PUTEFF	MACRO     PX
		DB	16,PX
	ENDM

	;*********************************************************
	;	MACRO TO BUILD CHARGE LETTERING TABLE ENTRY
	;*********************************************************

	PUTCHG	MACRO
		DB	17
	ENDM

	;*********************************************************
	;   MACRO TO BUILD BOUNTY BOB, NUMBER OF CATWALKS,
	;   AND TIMER TABLE ENTRIES
	;	PX    - X COORDINATE
	;	PY    - Y COORDINATE
	;	PJ    - NUMBER OF CATWALKS
	;	TIME  - TIMER VALUE
	;*********************************************************

	PUTBOB	MACRO	   PX,PY,CATS,TIME
		DB	18,PX,PY,CATS,TIME
	ENDM

	;*********************************************************
	;   MACRO TO BUILD TABLE END TABLE ENTRY
	;*********************************************************

	PUTEND	MACRO
		DB	0
	ENDM
