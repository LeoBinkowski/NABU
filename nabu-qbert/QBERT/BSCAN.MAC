FINDNEXT::
	LD A,0
	LD (COUNT),A		;RESET START COUNTER TO 0
	LD HL,BLOCK		;START ADDRESS OF BLOCK
LOOP:
	LD A,(HL)		;3.5 USEC
	LD B,A			;2
	INC HL			;3
	LD A,(HL)		;3.5		TOTAL 22.5
	LD C,A			;2
	INC HL			;3
	LD A,(HL)		;3.5
	LD D,A			;2
SIX1:
	LD E,1			;3.5
	LD A,B			;2
	AND 0FCH		;3.5		TOTAL 17.5
	CP 0FCH			;3.5
	CALL Z,FOUND		;CONDITION TRUE 8.5 OTHERWISE 5
SIX2:
	LD E,2			;3.5
	LD A,C			;2
	AND F0H			;3.5
	CP F0H			;3.5
	JP NZ,SIX3		;5		TOTAL 31.5
	LD A,B			;2
	AND 02H			;3.5
	CP 02H			;3.5
	CALL Z,FOUND		;5
SIX3:
	LD E,3			;3.5
	LD A,C			;2
	AND 0FH			;3.5
	CP 0FH			;3.5
	JP NZ,SIX4		;5		TOTAL 31.5
	LD A,D			;2
	AND 0C0H		;3.5
	CP 0C0H			;3.5
	CALL Z,FOUND		;5
SIX4:
	LD E,4			;3.5
	LD A,D			;2
	AND 3FH			;3.5
	CP 3FH			;3.5		TOTAL 25.5
	CALL Z,FOUND		;5
	INC HL			;3
	JP LOOP			;5

;LOOP EXECUTED 16K/3. LOOP HAS 128.5 USECS @ 2 MEG
;TOTAL TIME IN MICROSECS FOR THIS LOOP IS 16K/3*128.5/1000000 * 2/3.5 = .46785
;
FOUND:
	LD A,(COUNT)		;6.5
	INC A			;2
	CP 100			;3.5
	RET NZ			;6.5
;
;THIS LOOP EXECUTED MAX 100 TIMES
;TOTAL TIME 100*18.5/1000000 * 2/3.5 = 0.00105
;
	POP IX			;IF END IS MET POP OF RETURN INTO LOOP
	RET			;RETURN TO WHERE CALLED FROM MAINLINE
